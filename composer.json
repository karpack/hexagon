{
    "name": "karpack/hexagon",
    "description": "Hexagonal architecture support module",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/karpack/hexagon/issues",
        "source": "https://github.com/karpack/hexagon"
    },
    "keywords": ["laravel", "design-pattern"],
    "authors": [
        {
            "name": "Kalesh Kaladharan",
            "email": "kalesh.13@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/contracts": ">=7.0",
        "illuminate/database": ">=7.0",
        "illuminate/queue": ">=7.0",
        "illuminate/support": ">=7.0",
        "karpack/contracts": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Karpack\\Hexagon\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "Karpack\\Hexagon\\HexagonServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}